{
   "manifest_version" : 2,
   "description": "Moves tabs so they are always sorted in most recently used (MRU) order, from left to right within each window.",
   "icons": {
      "128": "icon_128.png",
      "48": "icon_48.png"
   },
   "name": "Tab Stack Junk",
   "options_page": "options.html",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.1",
   "background" : {
      "scripts" : ["background.js"],
      "persistent" : false
   }
}
